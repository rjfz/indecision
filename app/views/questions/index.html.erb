<% if @questions %>
<% @questions.each do |question| %>
  <p>UUID: <%= question.id %></p><br>
  <p>Name: <%= question.title %></p><br>
  <%= link_to 'New lobby', new_lobby_path(question_id: question.id), class: "inline-flex items-center px-2.5 py-1.5 border border-transparent text-xs font-medium rounded shadow-sm text-white bg-indigo-600 hover:bg-indigo-700 focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-indigo-500" %>
  <hr>
<% end %>
<% else %>
<% "hey" %>
<% end %>
