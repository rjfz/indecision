<% @user.each do |user| %>
  <p>UUID: <%= user.email %></p><br>
  <p>Role: <%= user.role.name %></p><br>
  <%= link_to 'edit user', edit_user_path(id: user.id), class: "inline-flex items-center px-2.5 py-1.5 border border-transparent text-xs font-medium rounded shadow-sm text-white bg-indigo-600 hover:bg-indigo-700 focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-indigo-500" %>

  <hr>
<% end %>
