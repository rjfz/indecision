<p class="text-3xl text-center font-mono font-bold	tracking-wide	">Response Times</p>

<div class="w-1/2 h-1/2 mx-auto">
<canvas
  data-controller="chart"
  data-chart-type-value='line'
  data-chart-data-value='{ "labels": <%= steps.keys %>, "datasets": [{ "label": "Response Time", "backgroundColor": "transparent", "borderColor": "#00FF4C", "data": <%= steps1.values %> }, { "label": "Correct Response Time", "backgroundColor": "transparent", "borderColor": "#3B82F6", "data": <%= steps.values %> } ] }'
></canvas>
</div>

